{
  "public": ".",
  "trailingSlash": true,
  "rewrites": [
    { "source": "**", "destination": "/index.html" }
  ],
  "headers": [
    {
      "source": "**/*.js",
      "headers": [
        { "key": "Cache-Control", "value": "max-age=0" }
      ]
    },
    {
      "source": "**/*.css",
      "headers": [
        { "key": "Cache-Control", "value": "max-age=0" }
      ]
    }
  ],
  "directoryListing": false,
  "unlisted": [
    ".git",
    ".gitignore"
  ]
} 